{
  "name": "Findologic",
  "description": "Findologic search plugin",
  "namespace": "Findologic",
  "author": "AdeoWeb",
  "type": "template",
  "version": "0.0.7",
  "serviceProvider": "Findologic\\Providers\\FindologicServiceProvider",
  "dataProviders":
  [
    {
      "key"           :"Findologic\\Containers\\SearchFilterContainer",
      "name"          :"Filters",
      "description"   :"Display Findologic filters",
      "defaultLayoutContainer": "Search.Filter"
    },
    {
      "key"           :"Findologic\\Containers\\SearchBarContainer",
      "name"          :"Search Bar",
      "description"   :"Display search bar customized for Findologic",
      "defaultLayoutContainer": "Search.SearchBar"
    }
  ],
  "dependencies": {
    "findologic/http_request2": "2.3.1"
  }
}
