{
  "name": "findologic/plugin-plentymarkets-api",
  "type": "project",
  "description": "Findologic api plugin for plentymarkets system",
  "require":
  {
    "php": "^7.0",
    "plentymarkets/plugin-interface": "dev-beta7",
    "findologic/http_request2": "2.3.1",
    "plentymarkets-local/plugin-io": "1.0.1",
    "plentymarkets-ceres/plugin-ceres": "1.0.4"

  },
  "require-dev": {
    "phpunit/phpunit": "~5.6"
  },
  "repositories": {
    "plentymarkets-local": {
      "type": "package",
      "package": {
        "name": "plentymarkets-local/plugin-io",
        "version": "1.0.1",
        "source": {
          "url": "https://github.com/plentymarkets/plugin-io",
          "type": "git",
          "reference": "origin/development"
        }
      }
    },
    "plentymarkets-ceres": {
      "type": "package",
      "package": {
        "name": "plentymarkets-ceres/plugin-ceres",
        "version": "1.0.4",
        "source": {
          "url": "https://github.com/plentymarkets/plugin-ceres",
          "type": "git",
          "reference": "origin/development"
        }
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Findologic\\": "src/"
    }
  }
}
